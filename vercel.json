{
    "version": 2,
    "builds": [
      {
        "src": "public/*",
        "config": { "includeFiles": ["public/**"] }
      },
      {
        "src": "**/*.njk",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "public/bundle.js"
        }
      ]
  }