{
  "version": 2,
  "rewrites": [{ "source": "http://lvh.me", "destination": "https://skill-notes-orcin.vercel.app/" }],
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["views/**", "public/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}