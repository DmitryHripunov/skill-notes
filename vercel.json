{
    "version": 2,
    "builds": [
      {
        "src": "public/bundle.js",
        "use": "@vercel/node"
      },
      {
        "src": "**/*.njk",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
      ]
  }